server {
        listen 80;
        listen [::]:80;

        root /var/www/html;
        index index.html index.htm index.nginx-debian.html;

        server_name nodejs.korede.me www.nodejs.korede.me;

        location / {
                proxy_pass http://nodejs:5555;
        }

        location ~ /.well-known/acme-challenge {
                allow all;
                root /var/www/html;
        }
}